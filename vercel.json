{
    "version": 2,
    "builds": [
        {
            "src": "index.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/sw.js",
            "headers": {
                "Cache-Control": "public, max-age=0, must-revalidate",
                "Service-Worker-Allowed": "/"
            }
        },
        {
            "src": "/(.*\\.css)",
            "headers": {
                "Content-Type": "text/css",
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/(.*\\.js)",
            "headers": {
                "Content-Type": "application/javascript",
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}